    "class Rectangle:\n",
    "    def set_params(self, a, b):\n",
    "        self.a = a\n",
    "        self.b = b\n",
    "    def calc_surface(self):\n",
    "        return self.a*self.b\n",
    "\n",
    "r = Rectangle()\n",
    "r.set_params(5, 6)\n",
    "print(r.calc_surface())\n",
    "\n"
   ]
